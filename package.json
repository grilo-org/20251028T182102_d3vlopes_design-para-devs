{
  "name": "design-para-devs",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": ">= 9.0",
    "yarn": "npm"
  },
  "scripts": {
    "build": "next build",
    "build-storybook": "storybook build",
    "check": "npx @biomejs/biome check --apply --error-on-warnings ./src",
    "codegen": "graphql-codegen --config codegen.ts",
    "dev": "next dev",
    "generate": "npx plop --plopfile plop/plopfile.mjs",
    "generate:component": "npm run generate component",
    "postinstall": "lefthook install",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:ci": "npm run test --coverage",
    "test:staged": "npm run test -- -o",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm run test -- --watch",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@splidejs/react-splide": "^0.7.12",
    "@vercel/speed-insights": "^1.0.10",
    "clsx": "^2.1.0",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "lucide-react": "^0.334.0",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-photoswipe-gallery": "^3.0.1",
    "sanitize-html": "^2.12.1",
    "tailwind-merge": "^2.2.1",
    "tailwind-variants": "^0.2.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "4.2.4",
    "@storybook/addon-designs": "^7.0.9",
    "@storybook/addon-essentials": "^7.6.16",
    "@storybook/addon-interactions": "^7.6.16",
    "@storybook/addon-links": "^7.6.16",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/addon-styling-webpack": "^0.0.6",
    "@storybook/blocks": "^7.6.16",
    "@storybook/nextjs": "^7.6.16",
    "@storybook/react": "^7.6.16",
    "@storybook/test": "^7.6.16",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/sanitize-html": "^2.11.0",
    "autoprefixer": "^10.0.1",
    "css-loader": "^6.10.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lefthook": "^1.6.1",
    "plop": "^4.0.1",
    "postcss": "^8",
    "postcss-loader": "^8.1.0",
    "storybook": "^7.6.16",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5"
  }
}
